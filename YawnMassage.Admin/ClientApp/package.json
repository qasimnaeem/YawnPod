{
  "name": "aldo-web",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.3.1",
    "@progress/kendo-data-query": "1.5.0",
    "@progress/kendo-dateinputs-react-wrapper": "2018.3.911",
    "@progress/kendo-drawing": "1.5.7",
    "@progress/kendo-react-dateinputs": "1.3.0",
    "@progress/kendo-react-dialogs": "1.3.0",
    "@progress/kendo-react-dropdowns": "1.3.0",
    "@progress/kendo-react-grid": "1.3.0",
    "@progress/kendo-react-inputs": "1.3.0",
    "@progress/kendo-react-intl": "1.3.0",
    "@progress/kendo-react-pdf": "1.3.0",
    "@progress/kendo-theme-bootstrap": "2.13.8",
    "@progress/kendo-ui": "2018.3.1003",
    "@progress/kendo-upload-react-wrapper": "^2018.3.1017",
    "axios": "0.18.0",
    "bootstrap": "4.1.3",
    "history": "4.7.2",
    "jquery": "3.3.1",
    "js-cookie": "2.2.0",
    "moment": "^2.22.2",
    "node-sass": "7.0.0",
    "node-sass-chokidar": "1.3.4",
    "npm-run-all": "4.1.1",
    "query-string": "6.1.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-notification-alert": "0.0.7",
    "react-redux": "5.0.7",
    "react-router-dom": "4.2.2",
    "react-router-dynamic-breadcrumbs": "2.1.1",
    "react-scripts-ts": "2.17.0",
    "react-spinners": "0.4.5",
    "reactstrap": "6.4.0",
    "redux": "4.0.0",
    "redux-form": "7.4.2",
    "redux-thunk": "2.3.0",
    "uuid": "^3.3.2"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-ts": "set PORT=4200 && react-scripts-ts start",
    "start": "npm-run-all -p --max-old-space-size=8192 watch-css start-ts",
    "build": "npm run build-css && react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/history": "^4.7.0",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.4",
    "@types/query-string": "^6.1.0",
    "@types/react": "^16.4.13",
    "@types/react-dom": "^16.0.7",
    "@types/react-redux": "^6.0.6",
    "@types/react-router-dom": "^4.3.0",
    "@types/reactstrap": "^6.0.2",
    "@types/redux-thunk": "^2.1.0",
    "typescript": "^3.0.3",
    "@types/redux-form": "^7.4.7"
  }
}
